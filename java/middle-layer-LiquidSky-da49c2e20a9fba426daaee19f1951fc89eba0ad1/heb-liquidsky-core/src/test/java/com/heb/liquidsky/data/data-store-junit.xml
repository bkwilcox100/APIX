<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<data-store>
	<!-- Sample code for JUnit -->
	<data-type name="test-product" id-property="id" id-generator-prefix="test-prod" pub-sub-enabled="false" fcm-enabled="false">
		<custom-property name="pageTitle" property-type="java.lang.String" source="com.heb.liquidsky.data.custom.PrependMessageProperty">
			<attribute name="message" value="HEB - " />
			<attribute name="property" value="title" />
		</custom-property>
		<table name="test_product" type="primary" id-column="prod_id">
			<column column-name="prod_id" property="id" />
			<column column-name="title" property="title" >
				<attribute name="test" value="42"/>
				<attribute name="minLength" value="1"/>
			</column>
		</table>
		<table name="test_product_aux" type="auxiliary" id-column="prod_id">
			<column column-name="description" property="description" />
		</table>
		<table name="test_sku" type="reference" id-column="required_prod_id">
			<column column-name="sku_id" list-item-type="test-sku" property="requiredSkus" read-only="true" cascade="true" />
		</table>
		<table name="test_sku" type="reference" id-column="optional_prod_id">
			<column column-name="sku_id" list-item-type="test-sku" property="optionalSkus" />
		</table>
		<table name="test_related_products" type="auxiliary" id-column="prod_id">
			<column column-name="related_prod_id" list-item-type="test-product" property="childRelatedProducts" />
		</table>
		<table name="test_related_products" type="auxiliary" id-column="related_prod_id">
			<column column-name="prod_id" list-item-type="test-product" property="parentRelatedProducts" />
		</table>
	</data-type>

	<data-type name="test-sku" id-property="id" id-generator-prefix="test-sku" pub-sub-enabled="false" fcm-enabled="false">
		<table name="test_sku" type="primary" id-column="sku_id">
			<column column-name="sku_id" property="id" />
			<column column-name="title" property="title" />
			<column column-name="required_prod_id" item-type="test-product" property="requiredProduct" />
			<column column-name="optional_prod_id" item-type="test-product" property="optionalProduct" />
		</table>
		<table name="test_sku_aux" type="auxiliary" id-column="sku_id">
			<column column-name="description" property="description" />
		</table>
	</data-type>
</data-store>
